generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model todos {
  id      Int      @id(map: "tasks_pkey") @default(autoincrement())
  task    String
  author  String
  state   String
  created DateTime @default(now()) @db.Timestamp(6)
}
